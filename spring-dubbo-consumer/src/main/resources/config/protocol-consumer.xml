<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--XML生产者应用信息-->
    <dubbo:application name="spring-dubbo-consumer" logger="log4j2"/>

    <!--Zookeeper注册中心配置-->
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="localhost:2181"  check="true"/>

    <!--Dubbo服务注入-->
    <dubbo:reference id="dubboTestService" interface="com.zsl.service.protocol.DubboProtocolService"
                     protocol="dubbo" registry="zookeeper" check="true"/>

</beans>